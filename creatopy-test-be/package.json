{
  "name": "node-ts-sequalize-boilerplate",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "tsc && node dist/app.js",
    "db": "sequelize db:migrate:status",
    "dev": "nodemon ./app.js",
    "test": "NODE_ENV=test mocha test/**/*.spec.js",
    "test-unit": "NODE_ENV=test mocha --exit ./test/unit/**/*.js",
    "test-integration": "NODE_ENV=test mocha --exit ./test/integration/**/*.js",
    "watch-tests": "NODE_ENV=test mocha --exit --watch ./test/{unit,integration}/index.js",
    "eslint": "eslint ./server --fix",
    "build": "tsc",
    "copyMySpecialFiles": "copyfiles --error --up 1 server/templates/*.* dist/server",
    "postbuild": "yarn run copyMySpecialFiles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
