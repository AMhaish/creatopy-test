    "type": "git",
    "url": "git+https://github.com/AMhaish/node-ts-sequalize-boilerplate"
  },
  "homepage": "https://github.com/AMhaish/node-ts-sequalize-boilerplate#readme",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
