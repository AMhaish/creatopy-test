    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "copy-webpack-plugin": "^9.0.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "easygraphql-tester": "^5.1.6",
    "events": "^3.2.0",
    "express": "^4.16.4",
    "file-type": "^16.5.0",
